close all; clear all; clc

syms s; % m k b f;

% Mass = 5;               % m
m = 5;
% SpringK = 1;            % k
k = 1;
% DampingK = 0.5;         % b
b = 0.5;
InputForce = 2;         % F

A =([0 1; -k/m -b/m]);  B =([0; 1/m]); C =([1 0; 0 0]); D =([0; 0]); 
% -k/m -b/m]);
phi = inv((s.*eye(2))-A);                 H = C*phi*B+D;
pretty(simplify(H));

mytf = tf([0 0 2], [10 1 2]);  mytf2 = tf([0 0 0 2], [10 2 1 0]);
% step(mytf);
% impulse(mytf);
step(mytf2);
